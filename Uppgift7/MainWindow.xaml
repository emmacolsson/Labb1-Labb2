        <TextBox x:Name="txtBox4" HorizontalAlignment="Left" Margin="144,373,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140" Height="38" HorizontalContentAlignment="Left" VerticalContentAlignment="Bottom" FontSize="16"/>
        <Button x:Name="btnCount" Content="Beräkna" HorizontalAlignment="Left" Margin="144,214,0,0" VerticalAlignment="Top" Height="39" Width="97" FontSize="16" Click="btnCount_Click" />
        <Label Content="Tal 1" HorizontalAlignment="Left" Margin="144,109,0,0" VerticalAlignment="Top" Height="31" Width="59" FontSize="16"/>
        <Label Content="Tal 2" HorizontalAlignment="Left" Margin="330,107,0,0" VerticalAlignment="Top" Height="31" Width="59" FontSize="16"/>
        <Label Content="Heltal" HorizontalAlignment="Left" Margin="144,263,0,0" VerticalAlignment="Top" Height="31" Width="59" FontSize="16"/>
        <Label Content="Rest" HorizontalAlignment="Left" Margin="144,342,0,0" VerticalAlignment="Top" Height="31" Width="59" FontSize="16"/>

    </Grid>
</Window>
