<Window x:Class="Uppgift7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift7"
        mc:Ignorable="d"
        Title="Heltalsdivision" Height="650" Width="800">
    <Grid>
        <TextBox x:Name="txtBox1" HorizontalAlignment="Left" Margin="144,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="141" Height="37" HorizontalContentAlignment="Left" VerticalContentAlignment="Bottom" FontSize="16"/>
        <TextBox x:Name="txtBox2" HorizontalAlignment="Center" Margin="0,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140" Height="37" HorizontalContentAlignment="Left" VerticalContentAlignment="Bottom" FontSize="16"/>
